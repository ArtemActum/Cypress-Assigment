{
  "name": "cypress-assigment",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cypress:execution": "cypress run",
    "generate:report": "node ./cucumber-html-report.js",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:all": "cypress run -s cypress/e2e/tests/*.cy.js",
    "cy:run:login": "cypress run -s cypress/e2e/tests/login.cy.js",
    "cy:run:search": "cypress run -s cypress/e2e/tests/search.cy.js",
    "cy:run:lang": "cypress run -s cypress/e2e/tests/languageSwitcher.cy.js",
    "cucumber:tests": "cypress run -s cypress/e2e/cucumber-tests/features/*.feature",
    "cucumber:lang": "cypress run --spec cypress/e2e/cucumber-tests/features/languageSwitcher.feature"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^12.0.1",
    "multiple-cucumber-html-reporter": "^1.21.6",
    "prettier": "^2.7.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.3",
    "@cucumber/cucumber": "^8.5.1",
    "@cypress/webpack-preprocessor": "^5.12.0",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.0",
    "chai-style": "^1.0.3",
    "cucumber-html-reporter": "^5.5.0",
    "cypress": "^10.3.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-esbuild-preprocessor": "^1.0.2",
    "cypress-mochawesome-reporter": "^3.2.0",
    "esbuild": "^0.14.50",
    "eslint": "^8.20.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1"
  }
}
